<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.vibecheck.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/home_screen" />

    <fragment
        android:id="@+id/navigation_login"
        android:name="com.example.vibecheck.ui.login.LoginFragment"
        android:label="@string/title_login"
        tools:layout="@layout/login" />

    <fragment
        android:id="@+id/navigation_signup"
        android:name="com.example.vibecheck.ui.signup.SignupFragment"
        android:label="@string/title_signup"
        tools:layout="@layout/signup" />




</navigation>